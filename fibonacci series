package fib;
import java.util.Scanner;

public class fib {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a number");
		int n = sc.nextInt();
		int i,f=0,x=1,a=0;
		for (i = 1; i <= n; i++) {
			
			if (a <= n) {
				System.out.println(a);
				a = f + x;
				f=x;
				x=a;
				if (i < 2) {
					System.out.println(i);
				}
			}
			
		}
	}
}
